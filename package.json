{
  "name": "pad-games",
  "version": "0.0.2",
  "private": true,
  "author": {
    "name": "Matthew Carlson",
    "url": "https://matthewfcarlson.github.io"
  },
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/matthewfcarlson/padgames/issues"
  },
  "repository": "github:matthewfcarlson/padgames",
  "description": "Games based around vue, websockets, and bringing people together",
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^1.0.1",
    "brain.js": "^1.6.1",
    "chart.js": "^2.8.0",
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "javascript-state-machine": "^3.1.0",
    "mongoose": "^5.6.11",
    "random-js": "^1.0.8",
    "register-service-worker": "^1.6.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "underscore": "^1.9.1",
    "velocity-animate": "^1.5.2",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-class-component": "^7.0.2",
    "vue-directive-touch": "^1.0.28",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-scroll": "^2.1.12",
    "vue-socket.io": "^3.0.7",
    "vuex": "^3.1.1"
  },
  "main": "dist_server/main.js",
  "scripts": {
    "e2e": "node test/e2e/runner.js",
    "test": "jest --config test/unit/jest.conf.js",
    "server-clean": "rimraf dist_server/*",
    "client-clean": "rimraf dist_client/*",
    "clean": "npm-run-all server-clean client-clean",
    "preclient-build": "tslint -c tslint.json -p client --fix",
    "preserver-build": "tslint -c tslint.json -p server --fix",
    "client-lint": "tslint -c tslint.json -p client/tsconfig.json --fix",
    "server-lint": "tslint -c tslint.json -p server/tsconfig.json --fix",
    "client-tsc": "tsc -p client",
    "server-tsc": "tsc -p server",
    "client-build": "npm-run-all client-clean client-lint client-tsc",
    "server-build": "npm-run-all server-clean server-lint server-tsc",
    "server-dev": "nodemon --watch server -e ts,ejs --exec npm run server-build:dev",
    "client-dev": "nodemon --watch server -e ts,ejs --exec npm run client-build:start",
    "server-build:dev": "npm-run-all server-build server-dev",
    "client-build:dev": "npm-run-all client-build client-dev",
    "server-start": "node .",
    "server": "npm run server-build:dev",
    "start": "npm-run-all client-build:dev server-build:dev"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "devDependencies": {
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^23.1.4",
    "@types/node": "^12.7.2",
    "@types/shelljs": "^0.8.5",
    "@types/socket.io": "^2.1.2",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prerender-spa-plugin": "^3.2.1",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "ts-jest": "^23.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.4.3"
  }
}
